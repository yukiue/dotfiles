! Rxvt
Rxvt*geometry:			80x35
Rxvt*foreground:		white
Rxvt*background:		black
Rxvt*cursorColor:		#BC6EC5 ! aquamarine1 
Rxvt*font:			xft:source code pro:regular:pixelsize=20,\
                xft:IPAExGothic :regular:pixelsize=20
Rxvt*boldFont:			xft:source code pro:semibold:pixelsize=20
Rxvt*loginShell:		on
Rxvt*visualBell:		on
Rxvt*scrollBar:			off
Rxvt*borderColor:		#ebb134

! 0=black, 1=red,  2=green,  3=yellow,  4=blue, 5=magenta,  6=cyan,  7=white


URxvt.depth: 32
! URxvt*foreground: #d0d0d0
! URxvt.background: [75]#000000
URxvt*color0: #2E3436
URxvt*color1: #CC0000
URxvt*color2: #4E9A06
URxvt*color3: #C4A000
URxvt*color4: #3465A4
URxvt*color5: #75507B
URxvt*color6: #06989A
URxvt*color7: #D3D7CF
URxvt*color8: #555753
URxvt*color9: #EF2929
URxvt*color10: #8AE234
URxvt*color11: #FCE94F
URxvt*color12: #729FCF
URxvt*color13: #AD7FA8
URxvt*color14: #34E2E2
URxvt*color15: #EEEEEC

! Emacs
Emacs.geometry:			80x54
Emacs.foreground:		white
Emacs.background:		#172727
Emacs.cursorColor:		aquamarine1
Emacs.font:			terminus-20
Emacs.toolBar:			off
Emacs.menuBar:			off
Emacs.verticalScrollBars:	off
Emacs.reverseVideo:		off

! https://github.com/simmel/urxvt-resize-font
urxvt.perl-ext-common: resize-font